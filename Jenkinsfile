pipeline{
    agents any
    stages{
        stage("Init"){
            steps{
                echo "This is the first step"
            }
        }
        stage("Build"){
            steps{
                echo "This is build stage"
                echo "Hello world"
            }
        }
        stage("Deploy"){
            steps{
                echo "This is deploying stage"
            }
        }
    }
}